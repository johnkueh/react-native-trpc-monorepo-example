generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}

model Greeting {
  id     Int     @id @default(autoincrement())
  message    String?
}

model User {
  id String @id() @default(cuid())
  email String?
  name String?
}